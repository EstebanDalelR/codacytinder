<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/mid_transparent_white"/>
            <corners android:topLeftRadius="@dimen/radius_corners" android:topRightRadius="@dimen/radius_corners" android:bottomLeftRadius="@dimen/radius_corners" android:bottomRightRadius="@dimen/radius_corners"/>
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <solid android:color="@color/white"/>
            <corners android:topLeftRadius="@dimen/radius_corners" android:topRightRadius="@dimen/radius_corners" android:bottomLeftRadius="@dimen/radius_corners" android:bottomRightRadius="@dimen/radius_corners"/>
        </shape>
    </item>
</selector>
